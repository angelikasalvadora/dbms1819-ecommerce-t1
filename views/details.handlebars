
	<br><br>



      <div class="container" >
          <div class="row">
      		<div class="col-md-4">
                  <div class="well well-sm">
                      <div class="media">
                          <a class="thumbnail pull-left" href="#">
                              <img  class="resize" src="http://www.icons101.com/icon_ico/id_79389/user.ico">
                          </a>
                          <div class="media-body">
                              <h4 class="media-heading">{{first_name}} {{last_name}}</h4>
                      		<p><span class="label label-info">{{street}}, {{municipality}}</span> <span class="label label-warning"> {{province}} {{zipcode}}</span></p>
                              <p>
                                  <a href="#" class="btn btn-xs btn-default"><span class="glyphicon glyphicon-comment"></span> {{email}} </a>

                              </p>
                          </div>
                      </div>
                  </div>
              </div>
      	</div>
      </div>


<style >
img.resize {
  width:50px;
  height:50px;
}
}
</style>
	<div class="container">
		<table class="table table-hover">
			<thead>
				<tr>
					<th class="text-center">
						<label>#</label>
					</th>
					<th class="text-center">
						<label>Product Name</label>
					</th>
					<th class="text-center">
						<label>Price</label>
					</th>
					<th class="text-center">
						<label>Quantity</label>
					</th>
					<th class="text-center">
						<label>Total</label>
					</th>
					<th class="text-center">
						<label>Date and Time</label>
					</th>

				</tr>
			</thead>
			<tbody>
				{{#each orders_data}}
					<tr>
						<td class="text-center">{{this.id}}</td>
						<td class="text-center">{{this.product_name}}</td>
						<td class="text-center">Php {{this.price}}</td>
						<td class="text-center">{{this.quantity}}</td>
						<td class="text-center">Php {{this.total}}</td>
							<td class="text-center">{{moment this.order_date local=true add='hours' amount='8' format='llll'}}</td>

					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
  <br><br>

<!-- Most Orders -->
<br>
<div class="container">
  <h3>Top 10 Costumers with Most Orders</h3>
  <table class="table table-hover">
    <thead>
      <tr>

        <th class="text-center">
          <label>Name</label>
        </th>
        <th class="text-center">
          <label>Email</label>
        </th>
        <th class="text-center">
          <label>Quantity</label>
        </th>

      </tr>
    </thead>
    <tbody>
      {{#each topTenCustomersWithMostOrders}}
        <tr>
          <td class="text-center">{{this.first_name}} {{this.last_name}}</td>
          <td class="text-center">{{this.email}}</td>
          <td class="text-center">{{this.number_of_orders}}</td>


        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<!-- Most Orders -->

